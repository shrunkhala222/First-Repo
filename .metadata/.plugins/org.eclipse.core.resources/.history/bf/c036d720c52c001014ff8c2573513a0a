package RINKU;

public class Array1 {

	public static void main(String[] args) {
	  int a[] =  new int [5];
	           a[0]= 10;
	           a[1]= 20;
	    	   a[2]= 30;
	    	   a[3]= 40;
	    	   a[4]= 50;
	    	   
	    	   
	    	   int a[] = {10,20,30,40,50};
	    	   
	    	   System.out.println(a.length);
	    			   
			 

	}

}
